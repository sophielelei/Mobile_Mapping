Title = {Intervention Now to Eliminate Repeat Unintended Pregnancy in Teenagers
   (INTERUPT): a systematic review of intervention effectiveness and
   cost-effectiveness, and qualitative and realist synthesis of
   implementation factors and user engagement},
Journal = {HEALTH TECHNOLOGY ASSESSMENT},
Year = {2016},
Volume = {20},
Number = {16},
