@article{ WOS:000221905700008,
Author = {Houry, D and Parramore, C and Fayard, G and Thorn, J and Heron, S and
   Kellermann, A},
Title = {Characteristics of household addresses that repeatedly contact 911 to
   report intimate partner violence},
