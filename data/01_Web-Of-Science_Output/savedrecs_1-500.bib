@article{ WOS:000968195900001,
Author = {Nkoa, Bruno Emmanuel Ongo and Song, Jacques Simon},
Title = {How digital innovation affects women's entrepreneurship in Africa? An
   analysis of transmission channels},
Journal = {INTERNATIONAL JOURNAL OF ENTREPRENEURSHIP AND INNOVATION},
Year = {2023},
Month = {2023 MAR 20},
Abstract = {Digital innovations in Africa have increased dramatically over the past
   two decades, and are fueling a rich literature. In this paper, we
   examine their impact and transmission channels on women's
   entrepreneurship in a sample of 54 African countries. We specify and
   estimate a cross-sectional data model by generalized least squares (GLS)
   over the period of 1996-2020. Our results show that digital innovations
   (measured by researchers in R\&D, firms in R\&D, patents owed by
   residents, patents owed by non-residents) significantly reduce women's
   entrepreneurship in Africa. Their effects are mediated by energy, female
   education, the internet, mobile phones, migration and marriage. We
   suggest strengthening the digital infrastructure for sustainable women's
   entrepreneurship in Africa.},
