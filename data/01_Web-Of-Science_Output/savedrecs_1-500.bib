@article{ WOS:000679812200024,
Author = {Diiro, Gracious M. and Fisher, Monica and Kassie, Menale and Muriithi,
   Beatrice W. and Muricho, Geoffrey},
Title = {How does adoption of labor saving agricultural technologies affect
   intrahousehold resource allocations? The case of push-pull technology in
   Western Kenya},
Journal = {FOOD POLICY},
Year = {2021},
Volume = {102},
Month = {JUL},
Abstract = {Considerable research documents why women farmers have lower technology
   adoption rates than men farmers, but relatively little is known about
   what happens within a household after technology uptake. This study
   contributes through an investigation of the intrahousehold distribution
   of benefits and costs of agricultural technology adoption in western
   Kenya. Using gender-disaggregated data and an endogenous switching
   regression approach, we elucidate the causal effects of push pull
   technology (PPT) adoption on intrahousehold labor and expenditure
   allocation. Results show that adoption increases household labor
   allocation for harvesting of maize, the staple crop, but reduces the
   labor required for other tasks (e.g., ploughing and weeding). In net,
   the technology is labor saving, with men experiencing a slightly greater
   workload reduction than women. In terms of expenditure impacts, PPT
   uptake increases household expenditures on children's education and
   consumption goods commonly associated with female preferences. Study
   findings support wider uptake of PPT to trigger gains in social and
   economic wellbeing for both men and women farmers. Implications for
   policy and practice are discussed.},
