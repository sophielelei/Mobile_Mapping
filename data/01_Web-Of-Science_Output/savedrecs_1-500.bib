@article{ WOS:000490689800001,
Author = {Tiwari, Jaya and Schaub, Emily and Sultana, Naziha},
Title = {Barriers to ?last mile? financial inclusion: cases from northern Kenya},
Journal = {DEVELOPMENT IN PRACTICE},
