@article{ WOS:001208385200011,
Author = {Schiffer, Anne and Greene, Mary and Khalid, Rihab and Foulds, Chris and
   Vidal, Cecilia alda and Chatterjee, Monolita and Dhar-bhattacharjee,
   Sunrita and Edomah, Norbert and Sule, Obehi and Palit, Debajit and
   Yesutanbul, Amos nkpeebo},
Title = {Brokering Gender Empowerment in Energy Access in the Global South},
