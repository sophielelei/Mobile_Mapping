@article{ WOS:000718065100001,
Author = {Mwalongo, Alcuin Ivor and Mkonongwa, Luka Mathayo},
Title = {Lecturers' Perceptions of Support for Integrating Information and
   Communication Technologies in Teaching and Learning},
Journal = {TECHNOLOGY KNOWLEDGE AND LEARNING},
Year = {2023},
