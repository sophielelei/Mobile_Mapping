@article{ WOS:000458929500013,
Author = {Van Hove, Leo and Dubus, Antoine},
Title = {M-PESA and Financial Inclusion in Kenya: Of Paying Comes Saving?},
Journal = {SUSTAINABILITY},
Year = {2019},
Volume = {11},
Number = {3},
Month = {FEB 1},
Abstract = {Mobile financial services such as M-PESA in Kenya are said to promote
   inclusion. Yet only 7.6 per cent of the Kenyans in the 2013 Financial
   Inclusion Insights dataset have ever used an M-PESA account to save for
   a future purchase. This paper uses a novel, three-step probit analysis
   to identify the socio-demographic characteristics of, successively,
   respondents who do not have access to a SIM card, have access to a SIM
   but do not have an M-PESA account, and, finally, have an account but do
   not save on it. We find that those who are excluded in the early stages
   are predominantly poor, non-educated, and female. For the final stage,
   we find that those who are in a position to save on their phonethe phone
   owners, the better educatedare less likely to do so. These results go
   against the traditional optimistic discourse on mobile savings as a
   prime path to financial inclusion. As such, our findings corroborate
   qualitative research that indicates that Kenyans have other needs, and
   want their money to circulate and work'.},
