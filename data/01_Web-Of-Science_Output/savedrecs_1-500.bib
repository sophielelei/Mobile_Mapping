@article{ WOS:000486770100001,
Author = {Afawubo, Komivi and Couchoro, Mawuli K. and Agbaglah, Messan and Gbandi,
   Tchapo},
Title = {Mobile money adoption and households' vulnerability to shocks: Evidence
   from Togo},
Journal = {APPLIED ECONOMICS},
Year = {2020},
Volume = {52},
Number = {10},
Pages = {1141-1162},
Month = {FEB 25},
Abstract = {We investigate the determinants of mobile money adoption process and
   whether its use helps households in Togo to be resilient to predictable
   and unpredictable life events. Using ordered logit and sequential logit
   models, our results show that in the adoption process, households
   benefit from weak ties of social groups such as religious group and
   informal saving group for the adoption of mobile money. We equally find
   that being client of banks or microfinance institutions act as powerful
   channels from one step to another in the process. Besides, our findings
   reveal that households whoever use mobile money seem to be more
   resilient to climatic shocks such as drought, irregular rain, soil
   degradation, erosion and fertility reduction and to shock that affect
   households' assets (non-climatic: high prices of agricultural inputs).
   However, the picture is more contrasted when the individuals are
   classified by disadvantaged groups such as rural people, women, less
   educated and people with low incomes.},
