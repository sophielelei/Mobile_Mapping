@article{ WOS:000435300300005,
Author = {Efobi, Uchenna R. and Tanankem, Belmondo V. and Asongu, Simplice A.},
Title = {Female Economic Participation with Information and Communication
   Technology Advancement: Evidence from Sub-Saharan Africa},
