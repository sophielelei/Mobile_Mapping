@article{ WOS:000405481000009,
Author = {Muita, Nduta and Diga, Kathleen},
Title = {Discontinuities in m-banking and development: Women who derive their
   livelihoods from agriculture in Maragua, Kenya A case study},
Journal = {AGENDA-EMPOWERING WOMEN FOR GENDER EQUITY},
Year = {2016},
Volume = {30},
Number = {4, SI},
Pages = {74-84},
Abstract = {In Africa about 70\% of women depend on agriculture as a key source of
   livelihoods through wages from labour and food production. It is evident
   that patriarchy and neoliberal policies prevent women from participating
   in the mainstream agricultural sector in Kenya. However, one rural
   community of women in Maragua, Kenya historically chose to challenge the
   policies of commodity crop planting through action and protest. We
   explore this same community in this period of the digital age, as to
   their resilient strategies to ensure that their families are fed. More
   specifically, the study critically assesses the financial strategy of
   mobile banking (m-banking) use among women who derive their livelihoods
   from the agricultural sector. This qualitative study is embedded within
   Sen's definition of development (1999), or expanding the freedoms that
   people themselves indicate as significant. Key findings from this study
   revealed that women did choose to use mobile phones for money services,
   yet the options of m-banking were limited. Their meagre incomes from
   farm produce made their m-banking use negligible and they preferred
   traditional collective savings practice. This case study argues that
   usage of mobile phones has not made a strong contribution to
   socio-economic change assisting women to fully participate in and
   transition towards enhanced rural agricultural development.},
