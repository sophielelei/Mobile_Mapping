@article{ WOS:001028803200042,
Author = {Abdelbagi, Abubakr and Literat, Ioana},
Title = {Mapping the Information Landscapes of Sudanese Youth: Implications for
   Media Literacy Education},
Journal = {INTERNATIONAL JOURNAL OF COMMUNICATION},
Year = {2023},
Volume = {17},
Pages = {4755-4777},
Abstract = {Seeking to expand the Western-centric scope of media literacy research,
   this article shines the spotlight on Sudan as a critically underexplored
   context. Using participatory mapping and in-depth interviews, our
   research foregrounds urban Sudanese youths' everyday informational
   resources and practices as a necessary foundation for a culturally
   responsive approach to media literacy education. We find that young
   people's information landscapes are heavily molded by the country's
   political context: Sudan's political instability and economic precarity
   increase the urgency and speed of information exchanges, while the
   prevalence of propaganda highlights the need for authentic, trustworthy
   information. This focus on usefulness and authenticity consequently
   shapes a preference for interpersonal communication and digital spaces
   as key informational channels. At the same time, our analysis reveals
   significant differences in young people's information landscapes,
   particularly along gender lines. Based on these findings, we outline key
   implications for culturally responsive media literacy interventions.},
