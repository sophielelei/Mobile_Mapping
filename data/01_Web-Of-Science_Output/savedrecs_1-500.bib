@article{ WOS:000232902600002,
Author = {Esilaba, AO and Byalebeka, JB and Delve, RJ and Okalebo, JR and
   Ssenyange, D and Mbalule, M and Ssali, H},
Title = {On farm testing of integrated nutrient management strategies in eastern
   Uganda},
Journal = {AGRICULTURAL SYSTEMS},
Year = {2005},
Volume = {86},
Number = {2},
Pages = {144-165},
Month = {NOV},
Abstract = {This paper reports on a Participatory Learning and Action Research
   (PLAR) process that was initiated in three villages in eastern Uganda in
   September 1999 to enable small-scale farmers to reverse nutrient
   depletion of their soils profitably by increasing their capacity to
   develop, adapt and use integrated natural resource management
   strategies. The PLAR process was also used to improve the participatory
   skills and tools of research and extension personnel to support this
   process. The farming systems of the area were characterised for
   socio-economic and biophysical conditions that included social
   organisations, wealth categories, gender, crop, soil, agro forestry and
   livestock production. Farmers identified soil fertility constraints,
   their indicators, and causes of soil fertility decline, and suggested
   strategies to address the problem of soil fertility decline. Soil
   fertility management diversity among households indicated that most
   farmers were not carrying out any improved soil fertility management
   practices, despite previous research and dissemination in the area.
   Following the diagnosis stage and exposure visits to other farmer groups
   working on integrated soil fertility projects, the farmers designed 11
   experiments for on-farm testing. One hundred and twenty farmers then
   chose, for participatory technology development, sub-sets of these 11
   experiments, based on the major agricultural constraints and the
   potential solutions identified and prioritised by the farmers.
   Quantitative and qualitative results from the testing, farmer evaluation
   and adaptation, training, dissemination strategies and socio-economic
   implications of these technologies are discussed. 2004 Elsevier Ltd. All
   rights reserved.},
