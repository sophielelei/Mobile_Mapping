@article{ WOS:000321083300003,
Author = {Djurfeldt, Agnes Andersson and Djurfeldt, Goran and Lodin, Johanna
   Bergman},
Title = {Geography of Gender Gaps: Regional Patterns of Income and Farm-Nonfarm
   Interaction Among Male- and Female-Headed Households in Eight African
   Countries},
Journal = {WORLD DEVELOPMENT},
Year = {2013},
Volume = {48},
Pages = {32-47},
Month = {AUG},
Abstract = {Many studies stress the existence of gender based income gaps across
   African production systems. Contextualizing such gaps in relation to
   regional characteristics, production systems, and nonfarm linkages
   challenges this. Household level data from 21 regions across eight
   African countries, collected in 2002 and 2008, are used to analyze
   production dynamics, market participation, and nonfarm linkages. Gender
   gaps are absent in 17 of the regions regardless of the overall regional
   income level. The results suggest that neither poverty nor growth in
   general discriminates against female headed households, but that causes
   of gender discrimination need to be found in specific regional contexts.
   (C) 2013 Elsevier Ltd. All rights reserved.},
Publisher = {PERGAMON-ELSEVIER SCIENCE LTD},
Address = {THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND},
Type = {Article},
