Book-Group-Author = {IEEE},
Title = {Assessing the Impact of Cell Phones on African Societies: An
   Investigation into the Impact of Cell Phones on a Community of Practice
   of Business Women in Nigeria},
Booktitle = {2018 IEEE 8TH ANNUAL COMPUTING AND COMMUNICATION WORKSHOP AND CONFERENCE
   (CCWC)},
