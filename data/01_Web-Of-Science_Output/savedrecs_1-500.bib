   Rachel and Brown, Derek S. and Atwebembere, Raymond and Mwebembezi, Abel},
Title = {Protocol for a discrete choice experiment: understanding preferences for
   seeking health services for survivors of sexual violence in Uganda},
Journal = {BMJ OPEN},
Year = {2024},
