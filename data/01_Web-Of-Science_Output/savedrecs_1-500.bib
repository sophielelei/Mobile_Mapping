@article{ WOS:000769634500001,
Author = {Kanda, Edwin Kimutai and Lutta, Valery Osimbo},
Title = {The status and challenges of a modern irrigation system in Kenya: A
   systematic review},
Journal = {IRRIGATION AND DRAINAGE},
