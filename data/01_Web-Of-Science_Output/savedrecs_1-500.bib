@inproceedings{ WOS:000860699400075,
Author = {Lubua, Edison Wazoel and Pretorius, Philip},
Book-Group-Author = {IEEE},
Title = {The Impact of Demographic Factors to the Adoption of Social Commerce in
   Tanzania},
Booktitle = {2018 IST-AFRICA WEEK CONFERENCE (IST-AFRICA)},
Year = {2018},
Note = {IST-Africa Week Conference (IST-Africa), Gaborone, BOTSWANA, MAY 09-11,
   2018},
Organization = {IST Africa},
Abstract = {This study determined the influence of demographic factors to the
   perceived relevance of social media-based commerce in Tanzania. The
   motivation comes from the recent trend where different service vendors
   take social networks as the platform for their business display, in
   conjunction to the use of mobile money services in business
   facilitation. The result is of urgency to assist these vendors,
   educators, policy makers and other stakeholders, to know how they can
   escalate the use of social media for income generation. The study used
   the literature to identify the gap of knowledge, and the hypothesis was
   formulated for testing. In addition, the study used advanced
   quantitative methods in its operationalisation, where analytical models
   such as the ordinal regression, chi-square, and ANOVA were applied.
   While the chi-square and ANOVA were used in determining the categorical
   relationship between demographic variables and the perceived relevance
   of social media based commerce, the study applied the Ordinal Regression
   model in deciding the impact of demographic variables to the perceived
   relevance of social media. Collectively, age and gender of respondents
   offered a significant impact to the perceived relevance of social media
   based commerce.},
