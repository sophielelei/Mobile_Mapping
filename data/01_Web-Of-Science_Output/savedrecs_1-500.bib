@article{ WOS:001173118000001,
Author = {Ndour, Cheikh Tidiane and Asongu, Simplice},
Title = {Information technology, gender economic inclusion and environment
   sustainability in sub-Sahara Africa},
Journal = {MANAGEMENT OF ENVIRONMENTAL QUALITY},
