@article{ WOS:000807276100001,
Author = {Hamdi, Yosr and Mulder, Nicola and Abdelhak, Sonia},
Title = {Women in Systems Science and Gender Parity: Why and How to Democratize
   the ``Technology, Innovation, and Society Nexus{''}},
