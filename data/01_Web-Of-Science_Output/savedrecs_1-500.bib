@article{ WOS:000704390800003,
Author = {Tambe, Telma Amorgiana Fulane and Miguel, Lucas Lavo Antonio Jimo},
Title = {Assessing emerging trends of expansion and women enrolments in the
   higher education system in Mozambique},
Journal = {WOMENS STUDIES INTERNATIONAL FORUM},
Year = {2021},
Volume = {88},
Month = {SEP-OCT},
Abstract = {We assessed the higher education (HE) record data including mission and
   vision statements from 2000 to 2015 to discuss emerging trends of
   expansion and women enrolments in Mozambique. Theoretical literature
   survey, Government decrees, laws, and strategic plans of higher
   education (SPHE) system between 2000-2010 and 2012-2020 supported the
   discussion. The SPHE impact on the HE expansion was tested using t-test,
   different variances, by comparing women's and men's enrolments, news
   admissions, and graduations in public and private institutions. Results
   obtained in public institutions in the field of social sciences,
   humanities, and services (SSHS) registered growth from 6383-83,571
   enrolments, 1246-20,570 new admissions, and 419-6723 graduations. In the
   field of Natural sciences, engineering, agriculture, and health (NSEAH)
   registered growth from 4850-32,466 enrolments, 300-6396 new admissions,
   and 259-2027 graduations. In private institutions, the SSHS registered
